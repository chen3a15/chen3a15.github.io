{
    "QQ音乐": {
        "星之阁（可用VIP）": {
            "url": "https://api.xingzhige.com/API/QQmusicVIP/",
            "args": {
                "request": {
                    "name": {
                        "必填": true,
                        "类型": "String",
                        "描述": "需要搜索的歌名",
                        "默认值": "八里香"
                    },
                    "uin": {
                        "必填": false,
                        "类型": "String",
                        "描述": "提供skey的账号(当站长提供的cookie失效时必填)",
                        "默认值": ""
                    },
                    "skey": {
                        "必填": false,
                        "类型": "String",
                        "描述": "提供开通有绿钻特权的skey可获取vip歌曲(当站长提供的cookie失效时必填)为空默认获取站长提供的skey",
                        "默认值": ""
                    },
                    "mode": {
                        "必填": false,
                        "类型": "String",
                        "描述": "接口模式。当填入QR时，为qr词库专用模式",
                        "默认值": ""
                    },
                    "type": {
                        "必填": false,
                        "类型": "String",
                        "描述": "返回格式，当模式为QR时生效，可选json,jsonCard,text，为空时默认text",
                        "默认值": "json"
                    },
                    "n": {
                        "必填": false,
                        "类型": "Int",
                        "描述": "用于返回指定序号的的歌曲信息",
                        "默认值": "1"
                    },
                    "max": {
                        "必填": false,
                        "类型": "Int",
                        "描述": "搜索上限，默认10，最大60",
                        "默认值": "10"
                    },
                    "br": {
                        "必填": false,
                        "类型": "Int",
                        "描述": "音质14为最高",
                        "默认值": "7"
                    },
                    "format": {
                        "必填": false,
                        "类型": "String",
                        "描述": "默认为空 等于lrc时输出lrc格式歌词",
                        "默认值": "lrc"
                    },
                    "mid": {
                        "必填": false,
                        "类型": "String",
                        "描述": "name参数为空时必要",
                        "默认值": ""
                    },
                    "songid": {
                        "必填": false,
                        "类型": "Int",
                        "描述": "歌曲id",
                        "默认值": ""
                    },
                    "song_type": {
                        "必填": false,
                        "类型": "Int",
                        "描述": "歌曲类型",
                        "默认值": ""
                    }
                },
                "return": {
                    "code": {
                        "类型": "Int",
                        "描述": "状态码"
                    },
                    "msg": {
                        "类型": "String",
                        "描述": "状态说明"
                    },
                    "songname": {
                        "类型": "String",
                        "描述": "歌名"
                    },
                    "name": {
                        "类型": "String",
                        "描述": "歌手名"
                    },
                    "pay": {
                        "类型": "String",
                        "描述": "是否收费"
                    },
                    "src": {
                        "类型": "String",
                        "描述": "播放直链，当提供的skey没有绿钻特权时，收费歌曲会获取失败"
                    },
                    "cover": {
                        "类型": "String",
                        "描述": "封面直链"
                    },
                    "songurl": {
                        "类型": "String",
                        "描述": "歌曲详情页"
                    }
                }
            }
        },
        "H深夜笔记（在做了*2）": {
